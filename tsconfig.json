{
    "compilerOptions": {
        "declaration": true,
        "target": "ES6",
        "module": "ESNext",
        "jsx": "react",
        "outDir": "./dist",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "baseUrl": "./",
      },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }